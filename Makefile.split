install-models-split: ## Install LLM models for split deployment
	@echo "$(BLUE)ğŸ¤– Installing LLM models...$(NC)"
	@docker-compose -f $(COMPOSE_FILE) exec ollama ollama pull llama3.2:3b 2>/dev/null || \
		docker-compose -f $(COMPOSE_FILE) exec ollama-gpu ollama pull llama3.2:3b 2>/dev/null || \
		echo "$(YELLOW)Ollama not available, skipping model install$(NC)"

install-chatterbox-split: ## Install Chatterbox TTS model for split deployment
	@echo "$(BLUE)ğŸ­ Installing Chatterbox TTS model...$(NC)"
	@echo "$(YELLOW)Note: Chatterbox will download on first TTS request$(NC)"

