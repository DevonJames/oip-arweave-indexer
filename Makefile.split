down-split: ## Stop all split services
	@echo "$(BLUE)ğŸ›‘ Stopping all split services...$(NC)"
	docker-compose -f $(COMPOSE_FILE) down
	@echo "$(GREEN)âœ… All services stopped$(NC)"

clean-split: ## Remove all split containers and volumes
	@echo "$(RED)âš ï¸  This will remove all containers and volumes for split deployment$(NC)"
	@read -p "Are you sure? [y/N] " confirm && [ "$$confirm" = "y" ] || exit 1
	docker-compose -f $(COMPOSE_FILE) down -v --remove-orphans
	@echo "$(GREEN)âœ… Cleanup complete$(NC)"

