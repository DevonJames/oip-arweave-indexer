logs-daemon: ## Show oip-daemon-service logs
	docker-compose -f $(COMPOSE_FILE) logs -f oip-daemon-service

logs-alexandria: ## Show alexandria-service logs
	docker-compose -f $(COMPOSE_FILE) logs -f alexandria-service

logs-both: ## Show both daemon and alexandria logs
	docker-compose -f $(COMPOSE_FILE) logs -f oip-daemon-service alexandria-service

restart-daemon: ## Restart oip-daemon-service
	@echo "$(BLUE)ğŸ”„ Restarting OIP Daemon...$(NC)"
	docker-compose -f $(COMPOSE_FILE) restart oip-daemon-service
	@echo "$(GREEN)âœ… Daemon restarted$(NC)"

restart-alexandria: ## Restart alexandria-service
	@echo "$(BLUE)ğŸ”„ Restarting Alexandria...$(NC)"
	docker-compose -f $(COMPOSE_FILE) restart alexandria-service
	@echo "$(GREEN)âœ… Alexandria restarted$(NC)"

shell-daemon: ## Shell into oip-daemon-service
	docker-compose -f $(COMPOSE_FILE) exec oip-daemon-service /bin/sh

shell-alexandria: ## Shell into alexandria-service
	docker-compose -f $(COMPOSE_FILE) exec alexandria-service /bin/sh

