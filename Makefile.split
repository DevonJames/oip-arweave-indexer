test-daemon: ## Test oip-daemon-service endpoints
	@echo "$(BLUE)ğŸ§ª Testing Daemon Service...$(NC)"
	@echo ""
	@echo "Testing basic health..."
	@curl -s http://localhost:3005/health | jq . || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "Testing records endpoint..."
	@curl -s "http://localhost:3005/api/records?limit=1" | jq '.total' || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "Testing GUN sync..."
	@curl -s http://localhost:3005/api/health/gun-sync | jq '.status' || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "Testing Elasticsearch..."
	@curl -s http://localhost:3005/api/health/elasticsearch | jq '.status' || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "$(GREEN)âœ… Daemon tests complete$(NC)"

test-alexandria: ## Test alexandria-service endpoints
	@echo "$(BLUE)ğŸ§ª Testing Alexandria Service...$(NC)"
	@echo ""
	@echo "Testing basic health..."
	@curl -s http://localhost:3006/health | jq . || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "Testing AI status..."
	@curl -s http://localhost:3006/api/health/ai | jq '.status' || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "Testing voice services..."
	@curl -s http://localhost:3006/api/health/voice | jq '.status' || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "Testing daemon connectivity..."
	@curl -s http://localhost:3006/api/health/daemon | jq '.status' || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "$(GREEN)âœ… Alexandria tests complete$(NC)"

test-integration: ## Test daemon-alexandria integration
	@echo "$(BLUE)ğŸ§ª Testing Service Integration...$(NC)"
	@echo ""
	@echo "Testing Alexandria â†’ Daemon proxying..."
	@echo "  Fetching records via Alexandria..."
	@curl -s "http://localhost:3006/api/records?limit=1" | jq '.total' || echo "$(RED)Proxy failed$(NC)"
	@echo ""
	@echo "Testing full health check..."
	@curl -s http://localhost:3006/api/health/full | jq . || echo "$(RED)Failed$(NC)"
	@echo ""
	@echo "$(GREEN)âœ… Integration tests complete$(NC)"

